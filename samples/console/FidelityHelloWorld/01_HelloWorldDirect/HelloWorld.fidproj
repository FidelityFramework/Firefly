[package]
name = "HelloWorldDirect"
version = "0.1.0"
description = "HelloWorld with direct module calls"
authors = ["SpeakEZ Technologies, Inc."]
license = "MIT"

[compilation]
memory_model = "stack_only"
target = "native"

[dependencies]
# Types and operations are FNCS intrinsics - no external library needed
platform = { path = "/home/hhh/repos/Fidelity.Platform/Linux_x86_64" }

[build]
sources = ["HelloWorld.fs"]
output = "helloworld"
output_kind = "console"
