[package]
name = "AddNumbers"
version = "0.1.0"
description = "Demonstrates discriminated unions, pattern matching, and SRTP arithmetic"
authors = ["SpeakEZ Technologies, Inc."]
license = "MIT"

[compilation]
memory_model = "stack_only"
target = "native"

[dependencies]
# Types and operations are FNCS intrinsics - no external library needed
platform = { path = "/home/hhh/repos/Fidelity.Platform/Linux_x86_64" }

[build]
sources = ["AddNumbers.fs"]
output = "addnumbers"
output_kind = "console"
